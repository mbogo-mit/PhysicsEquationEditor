          <% for(var i=0; i < loe.mechanics.length; i+=3){%>
            <div class="row my-row">

            <% if(i < loe.mechanics.length){%>
              <div class="col m4">
                <div class="row my-row">
                  <% if(loe.mechanics[i].info.videos.length > 0){%>
                  <div class="col m12 center static-physics-equation-header" infoId="<%= loe.mechanics[i].infoId %>" info="<%= JSON.stringify(loe.mechanics[i].info) %>">
                  <%}else{%>
                  <div class="col m12 center static-physics-equation-header">
                  <%}%>    
                      <span><%=loe.mechanics[i].name%></span>
                  </div>
                </div>
                <% for(let eq in loe.mechanics[i].equations){ %>
                  <div class="row my-row-2">
                    <div class="col m12 center">
                      <span class="static-physics-equation tooltipped" quantities="<%= JSON.stringify(loe.mechanics[i].quantities[eq])%>" data-position="left" data-tooltip="Copy" onclick="CopyPhysicsEquationToClipboard($(this))" latex="<%=loe.mechanics[i].equations[eq]%>"></span>
